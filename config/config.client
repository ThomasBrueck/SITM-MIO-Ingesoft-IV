#
# Configuración del Cliente ICE - Sistema MIO
#

# Configuración de proxy por defecto
Ice.Default.Locator=
Ice.Default.Router=

# Timeouts
Ice.Connection.ConnectTimeout=5000
Ice.Connection.IdleTimeout=60

# Logging
Ice.Warn.Connections=1
Ice.Trace.Network=0
Ice.Trace.Protocol=0

# Configuración de threading
Ice.ThreadPool.Client.Size=5
Ice.ThreadPool.Client.SizeMax=10
